{
  "name": "ai-agent-browser",
  "private": true,
  "version": "0.0.0",
  "main": "electron-dist/main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:vite\" \"npm run dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "npm run build:electron && electron . --disable-gpu --disable-software-rasterizer --disable-gpu-sandbox --no-sandbox",
    "build:electron": "tsc --project tsconfig.electron.json",
    "build": "vite build && npm run build:electron && electron-builder",
    "preview": "vite preview",
    "electron": "tsx electron/main.ts",
    "test": "tsx test-intelligent-routing.ts",
    "test:routing": "tsx test-intelligent-routing.ts",
    "debug": "node debug-fix.js",
    "debug:full": "node debug-script.js",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.0",
    "@google/genai": "^1.25.0",
    "@modelcontextprotocol/sdk": "^1.11.4",
    "@xenova/transformers": "^2.10.0",
    "cheerio": "^1.0.0-rc.12",
    "chrome-remote-interface": "^0.33.0",
    "ollama": "^0.5.0",
    "openai": "^4.28.0",
    "playwright-core": "^1.40.0",
    "puppeteer-core": "^21.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tesseract.js": "^5.0.0",
    "vectordb": "^0.4.0"
  },
  "devDependencies": {
    "@types/node": "^22.18.11",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^8.2.2",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.ai-agent-browser",
    "productName": "AI Agent Browser",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "node_modules/**/*"
    ],
    "mac": {
      "category": "public.app-category.productivity"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
